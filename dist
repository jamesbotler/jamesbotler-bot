function _interopRequireDefault(obj) { return obj && obj.__esModule ? obj : { default: obj }; }

async function run(str, lang) {
  const lines = str.split('.\n');
  let translation = '';

  for (const line of lines) {
    const hash = (0, _sha.default)(line);

    const dbPath = _path.default.join("./", "data", 'translations', `${hash}.db`);

    const db = _fs.default.existsSync(dbPath) ? JSON.parse(_fs.default.readFileSync(dbPath)) : {};
    if (!db.source) db.source = line;

    if (!db || !db[lang]) {
      db[lang] = await translate(line, lang);

      _fs.default.writeFileSync(dbPath, JSON.stringify(db));
    }

    if (db[lang].endWith('.')) {
      translation += `${db[lang]}\n`;
    } else {
      translation += `${db[lang]}.\n`;
    }
  }

  return translation;
}

async function translate(str, lang) {
  const response = await (0, _tunnelFetch.default)({
    method: "POST",
    url: _config.default.translation_api,
    data: {
      q: str
    },
    params: {
      source: "auto",
      target: lang
    }
  });

  if (!response || !response.status === 200 || !response.data || !response.data.translation) {
    console.log(response);
  }

  return response.data.translation;
}
